.header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 50rpx 50rpx 60rpx;
}
.topBG {
    background-color: #1bd0ad;
    height: 250rpx;
    position: relative;
    width: 100%;
    color: #fff;
}
.topImage {
    width: 100%;
    height: 250rpx;
    position: absolute;
    z-index: 0;
}
.header-right {
    width: 160rpx;
    height: 160rpx;
    border-radius: 50%;
    box-shadow: 0 0 10px #fff;
}

.header-right image {
    width: 140rpx;
    height: 140rpx;
    background: #fff;
    margin-top: 10rpx;
    border-radius: 50%;
}

.header-left {
    width: 60%;
    text-align: left;
}

.name {
    color: #fff;
    font-size: 45rpx;
}

.name view {
    width: 100%;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.city {
    color: #8d8d8d;
    font-size: 30rpx;
    margin-top: 15rpx;
}

.cell {
    margin-left: 50rpx;
    height: 100rpx;
    border-bottom: 1px solid rgba(230,230,230,0.6);
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.cell-left {
    display: flex;
    height: 100%;
    align-items: center;
}

.cell-icon {
    width: 52rpx;
    height: 52rpx;
}

.cell-icon image {
    width: 52rpx;
    height: 52rpx;
}

.cell-text {
    width: 390rpx;
    font-size: 35rpx;
    color: #414141;
    margin-left: 15rpx;
}

.cell-text-no {
    width: 550rpx;
}

.cell-text button,.cell-text button::after {
    background: none!important;
    border: none!important;
    text-align: left!important;
    padding: 0!important;
    font-size: 35rpx!important;
    color: #414141!important;
    width: 100%;
}

.cell-right {
    color: #8d8d8d;
    font-size: 30rpx;
    display: flex;
    align-items: center;
    margin-right: 50rpx;
}

.cell-right image {
    width: 16rpx;
    height: 24rpx;
    margin-left: 12rpx;
}

button::after,button {
    border: none;
    background: none;
}

.hidden-entrance {
    width: 200rpx;
    height: 200rpx;
    opacity: 0;
    position: relative;
    z-index: 99;
}